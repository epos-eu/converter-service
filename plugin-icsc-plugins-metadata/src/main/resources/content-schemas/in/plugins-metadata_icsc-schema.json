{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "ICS-C Converter Plugins Metadata",
	"description": "Content generated by the ICS-C DBConnector component describing the metadata for the available Converter plugins",
	"type": "array",
	"items": {
		"type": "object",
		"required": ["identifier", "downloadURL", "softwareVersion", "proxy-type", "requirements", "action", "operations"],
		"properties": {
			"identifier": {
				"type": "string"
			},
			"name": {
				"type": "string"
			},
			"downloadURL": {
				"type": "string"
			},
			"softwareVersion": {
				"type": "string"
			},
			"proxy-type": {
				"type": "string"
			},
			"requirements": {
				"type": "string"
			},
			"action": {
				"type": "object",
				"required": ["object", "result"],
				"properties": {
					"object": {
						"type": "object",
						"required": ["encodingFormat"],
						"properties": {
							"encodingFormat": {
								"type": "string"
							},
							"schemaVersion": {
								"type": "string"
							}
						},
						"additionalProperties": true
					},
					"result": {
						"type": "object",
						"required": ["encodingFormat"],
						"properties": {
							"encodingFormat": {
								"type": "string"
							},
							"schemaVersion": {
								"type": "string"
							}
						},
						"additionalProperties": true
					}
				}
			},
			"operations": {
				"type": "array",
				"items": {
					"type": "string"
				}
			},
			"location": {
				"type": "string"
			}
		},
		"additionalProperties": true
	}
}